def setup():

  size(500,500,P3D);
  noStroke();
  fill(255,190);

def draw():

  background(0);
  translate(250,250);
  rotateX(mouseX/200.0);
  rotateY(mouseY/100.0);
  
  for(int i = -height/2;i<height/2;i+=20)

    for(int j = -height/2;j<height/2;j+=20):
    
      rotateX(0.1);
      ellipse(i,j,30,30);
    
  
